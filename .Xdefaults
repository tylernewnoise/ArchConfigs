URxvt.geometry:			110x28
URxvt.borderless:		true

#transparency
#URxvt*inheritPixmap: true
URxvt*.transparent: true
URxvt*.shading: 23

#scrollbar
URxvt.scrollBar:		false
#URxvt.scrollstyle:		plain
URxvt.scrollBar_right:	true
URxvt.secondaryScroll:	true

#fonts
URxvt.urgentOnBell: true
URxvt.saveLines: 1000
#URxvt.font: xft:terminus:size=10:antialias=true
#URxvt.boldFont: xft:terminus Mono:bold:size=10:antialias=true
#URxvt*font: xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true,xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=ture
#URxvt*boldFont: xft:DejaVu Sans Mono:bold:size=10:antialias=false
#URxvt*font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12
URxvt*font: xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=ture


#Colorsceme from xcolors.net, theme gjm
URxvt*cursorColor:   #cee318
URxvt*background:    #1C1C1C
URxvt*foreground:    #c5c5c5
! black
URxvt*color0:        #1C1C1C
URxvt*color8:        #666666
! red
URxvt*color1:        #ff005b
URxvt*color9:        #ff00a0
! green
URxvt*color2:        #cee318
URxvt*color10:       #ccff00
! yellow
URxvt*color3:        #ffe755
URxvt*color11:       #ff9f00
! blue
URxvt*color4:        #048ac7
URxvt*color12:       #48c6ff
! magenta
URxvt*color5:        #833c9f
URxvt*color13:       #be67e1
! cyan 
URxvt*color6:        #0ac1cd
